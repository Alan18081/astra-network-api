{
  "name": "astra-store",
  "version": "0.0.0",
  "description": "Store platform where people can sell and buy products [Backend]",
  "author": "Alex",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "webpack": "webpack --config webpack.config.js --watch",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "5.1.0",
    "@nestjs/websockets": "^5.5.0",
    "@sendgrid/mail": "^6.3.1",
    "@types/lodash": "^4.14.118",
    "@types/multer": "^1.3.7",
    "@types/pug": "^2.0.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/uid-safe": "^2.1.2",
    "bcrypt": "^3.0.2",
    "card-validator": "^6.0.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cloudinary": "^1.13.2",
    "csurf": "^1.9.0",
    "elasticemail": "^0.2.0",
    "express-rate-limit": "^3.3.2",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "nodemailer": "^4.7.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.7.1",
    "pug": "^2.0.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.2.0",
    "stripe": "^6.15.1",
    "typeorm": "0.2.7",
    "typescript": "^3.0.3",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.4.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/stripe": "^6.0.4",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.3.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
